{
  "name": "ts-mean-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_ENV=production&&node ./dist/main.js",
    "start:client": "ng serve",
    "start:server": "ts-node --inspect --project ./server/tsconfig.json ./server/src/main.ts",
    "build:client": "ng build --prod",
    "build:server": "tsc --project ./server/src",
    "build": "run-s build:*",
    "watch:server": "nodemon",
    "watch:client": "ng build --watch",
    "watch": "run-p watch:*",
    "test:client": "ng test",
    "test:server": "jest --config server/src/jest.json",
    "lint:server": "tslint -c server/src/tslint.json -e 'node_modules/**' -e 'client/**' -e 'server/**/*.d.ts'  -e 'server/src/**/*.test.ts' 'server/src/**/*.ts'",
    "lint:client": "ng lint",
    "lint": "run-s lint:*",
    "e2e:client": "ng e2e",
    "e2e:server": "jest --config server/test/jest-e2e.json"
  },
  "private": true,
  "dependencies": {
    "@als/keyboard": "C:\\btree\\project\\als-libraries\\dist\\keyboard\\als-keyboard-1.0.0.tgz",
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.2.0",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/flex-layout": "6.0.0-beta.15",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.2.0",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@auth0/angular-jwt": "^2.0.0",
    "@nestjs/common": "^5.0.1",
    "@nestjs/core": "^5.0.1",
    "@nestjs/mongoose": "^5.0.0",
    "@nestjs/swagger": "^2.0.2",
    "@nestjs/testing": "^5.0.1",
    "@nestjs/typeorm": "^5.0.0",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "class-transformer": "^0.1.9",
