    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "server/src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
